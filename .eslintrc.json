{
    "extends": "./node_modules/@englercj/code-style/.eslintrc.json",
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "curly": [2, "all"],
        "max-lines": 0,
        "prefer-rest-params": 0,
        "no-magic-numbers": 0
    }
}
